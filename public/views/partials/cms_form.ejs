


<div class="back-office" id="cms">

        <script>
            var dataRevueWaitingFromDB = [];
            var dataRevueOnlineFromDB = [];
            var nodes = [];
        </script>
        
            <% data_online.forEach(function(o) { %>
                <!-- <script>dataRevueOnlineFromDB.push('{name: <%= o.name %>, revueID: <%= o.revueID %>, ID: <%= o.ID %>}');</script> -->
                <script>dataRevueOnlineFromDB.push({ID: '<%= o.ID %>', revueID: '<%= o.revueID %>', name: '<%= o.name %>',
                            link: '<%= o.link %>', year_start: '<%= o.year_start %>', year_end: '<%= o.year_end %>',
                            ongoing: '<%= o.ongoing %>', frequency: '<%= o.frequency %>', publisher: '<%= o.publisher %>',
                            city: '<%= o.city %>', country: '<%= o.country %>', lat: '<%= o.lat %>',
                            long: '<%= o.long %>', language: '<%= o.language %>', access: '<%= o.access %>', medium: '<%= o.medium %>',
                            about: '<%= o.about %>', peer_review: '<%= o.peer_review %>', note: '<%= o.note %>'
                },);</script>
            <% }); %>
            <% data_waiting.forEach(function(o) { %>
                <!-- <script>dataRevueWaitingFromDB.push('{name: <%= o.name %>, revueID: <%= o.revueID %>, ID: <%= o.ID %>}');</script> -->
                <script>dataRevueWaitingFromDB.push({ID: '<%= o.ID %>', revueID: '<%= o.revueID %>', name: '<%= o.name %>',
                    link: '<%= o.link %>', year_start: '<%= o.year_start %>', year_end: '<%= o.year_end %>',
                    ongoing: '<%= o.ongoing %>', frequency: '<%= o.frequency %>', publisher: '<%= o.publisher %>',
                    city: '<%= o.city %>', country: '<%= o.country %>', lat: '<%= o.lat %>',
                    long: '<%= o.long %>', language: '<%= o.language %>', access: '<%= o.access %>', medium: '<%= o.medium %>',
                    about: '<%= o.about %>', peer_review: '<%= o.peer_review %>', note: '<%= o.note %>'
                 },);</script>   <% }); %>
            <% nodes.forEach(function(o) { %>
                <!-- <script>nodes.push('{id: <%= o.id %>, parentNameID: <%= o.parentNameID %>, nameID: <%= o.nameID %>,name: <%= o.name %>}');</script> -->
                <script>nodes.push({id: '<%= o.id %>', parentNameID: '<%= o.parentNameID %>', nameID: '<%= o.nameID %>',name: '<%= o.name %>'});</script>
            <% }); %>
    
        <script>
            console.log("dataRevueOnlineFromDB",dataRevueOnlineFromDB);
